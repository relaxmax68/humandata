{# app/Resources/views/bootstrap.html.twig #}

<!DOCTYPE html>

<html>

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <title>{% block title %}{% endblock %}</title>

  {% block stylesheets %}
    <link rel="stylesheet" href="{{ asset('bootstrap.min.css') }}">
  {% endblock %}

  <link rel="icon" type="image/x-icon" href="{{ asset('humandataLogo.jpg') }}" />
  <link type="text/plain" rel="author" href="http://humandata.info/humans.txt" />

</head>

<body>

  <div class="container">
 
    {% block connexion %}
    {% endblock %}

    {% block body %}
    {% endblock %}

    {% block footer %}
    {% endblock %}

  </div>

  {% block javascripts %}
    {# Ajoutez ces lignes JavaScript si vous comptez vous servir des fonctionnalit√©s du bootstrap Twitter #}
    {# <script src="//ajax.googleapis.com/ajax/libs/jquery/2.2.0/jquery.min.js"></script> #}
    <script src="{{ asset('jquery-2.2.0.js')   }}"></script>
    <script src="{{ asset('bootstrap.min.js') }}"></script>
  {% endblock %}
</body>

</html>